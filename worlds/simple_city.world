            <time>220</time>
            <pose>15 -95 0 0 0 3.14</pose>
          </waypoint>
          <!-- Turn from -X to +Y -->
          <waypoint>
            <time>240</time>
            <pose>5 -85 0 0 0 1.57</pose>
          </waypoint>
        </trajectory>
      </script>
