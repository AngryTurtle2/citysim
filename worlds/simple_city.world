            <time>120</time>
            <pose>95 -15 0 0 0 -1.57</pose>
          </waypoint>
          <!-- Walk on -Y direction -->
          <waypoint>
            <time>160</time>
            <pose>95 -85 0 0 0 -1.57</pose>
          </waypoint>
          <!-- Turn from -Y to -X -->
