      <script>
        <trajectory id="0" type="animation">
          <!-- Starting pose -->
          <waypoint>
            <time>0</time>
            <pose>5 -85 0 0 0 1.57</pose>
          </waypoint>
          <!-- Walk on +Y direction -->
          <waypoint>
            <time>40</time>
            <pose>5 -15 0 0 0 1.57</pose>
          </waypoint>
          <!-- Turn from +Y to +X -->
          <waypoint>
            <time>60</time>
            <pose>15 -5 0 0 0 0</pose>
          </waypoint>
          <!-- Walk on +X direction -->
